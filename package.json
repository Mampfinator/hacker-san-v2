{
  "name": "hacker-san-v2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node -r ts-node/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm -- migration:run -d ./typeorm.config.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d ./typeorm.config.ts ./migrations/"
  },
  "dependencies": {
    "@discordjs/rest": "^1.7.0",
    "@nestjs-architects/typed-cqrs": "^1.0.0",
    "@nestjs-plus/discovery": "^2.0.2",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^0.27.2",
    "axios-cookiejar-support": "^4.0.6",
    "command-line-args": "^5.2.1",
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.1",
    "fast-xml-parser": "^4.0.8",
    "googleapis": "^118.0.0",
    "nanoid": "^4.0.0",
    "pg": "^8.8.0",
    "puppeteer": "^19.10.1",
    "puppeteer-tough-cookie-store": "^1.0.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "toml": "^3.0.0",
    "tough-cookie": "^4.1.2",
    "twitter-api-v2": "^1.12.2",
    "typeorm": "^0.3.9",
    "yt-scraping-utilities": "^0.1.6",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.1",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.11",
    "@types/command-line-args": "^5.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.13",
    "@types/supertest": "^2.0.11",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.35.1",
    "discord-api-types": "^0.37.39",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.3.2",
    "rimraf": "^5.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^5.0.4",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
